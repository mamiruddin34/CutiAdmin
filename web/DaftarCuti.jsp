                        <form role="form" action="DaftarCutiUmum" method="GET">
                            <!-- SECTION UNTUK INFO CUTI -->
                            <div class="form-inline">
                                <div class="form-group">
                                    <input type="text" class="form-control" rows="3" name="tahunCapai" placeholder="Tahun, etc: 2016"/>
                                </div>
